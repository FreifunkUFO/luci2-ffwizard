config autoconf autoconf
	option enabled 1
	option boot 1

config ffwizard ffwizard
	option hostname "OpenWRT"
	option enabled 0
	option vpn 0
	option bbvpn 1
	option br 1
	option br_ip "192.168.112.1/28"

config ether lan
	option enabled 0
	option device "lan"
	option olsr_mesh 1
	option bat_mesh 0

config ether wan
	option enabled 0
	option device "wan"
	option olsr_mesh 0
	option bat_mesh 0

config wifi wireless0
	option enabled 0
	option device "radio0"
	option olsr_mesh 1
	option bat_mesh 0
	option vap 1
	option mesh_ip "104.1.1.1/8"
	option vap_ip "192.168.111.1/28"

config wifi wireless1
	option enabled 0
	option device "radio1"
	option olsr_mesh 1
	option bat_mesh 0
	option mesh_ip "104.1.1.2/8"
	option vap 1
	option vap_br 1
